generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  uid         String  @unique
  email       String  @unique
  session     String  @unique
  photo       String?
  name        String?
  title       String?
  description String?
  githubUrl   String?
  youtubeUrl  String?
  linkedInUrl String?
  websiteUrl  String?

  @@map("users")
}
